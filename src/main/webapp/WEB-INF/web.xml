<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controllers.Authorization</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Main</servlet-name>
        <servlet-class>controllers.Main</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Main</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Profile</servlet-name>
        <servlet-class>controllers.Profile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Profile</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>controllers.Authentication</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Clubs</servlet-name>
        <servlet-class>controllers.controlpanel.ClubLogger</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Clubs</servlet-name>
        <url-pattern>/clubs</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ClubDetailed</servlet-name>
        <servlet-class>controllers.details.ClubDetailed</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClubDetailed</servlet-name>
        <url-pattern>/club</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Events</servlet-name>
        <servlet-class>controllers.controlpanel.EventLogger</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Events</servlet-name>
        <url-pattern>/events</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>News</servlet-name>
        <servlet-class>controllers.controlpanel.NewsLogger</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>News</servlet-name>
        <url-pattern>/news</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Users</servlet-name>
        <servlet-class>controllers.controlpanel.UserLogger</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Users</servlet-name>
        <url-pattern>/users</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EventDetailed</servlet-name>
        <servlet-class>controllers.details.EventDetailed</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EventDetailed</servlet-name>
        <url-pattern>/event</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Groups</servlet-name>
        <servlet-class>controllers.controlpanel.GroupLogger</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Groups</servlet-name>
        <url-pattern>/groups</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GroupDetailed</servlet-name>
        <servlet-class>controllers.details.GroupDetailed</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GroupDetailed</servlet-name>
        <url-pattern>/group</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetUsers</servlet-name>
        <servlet-class>controllers.controlpanel.GetUsers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetUsers</servlet-name>
        <url-pattern>/getusers</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteUsers</servlet-name>
        <servlet-class>controllers.controlpanel.RemoveUsers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteUsers</servlet-name>
        <url-pattern>/remove</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RemoveItem</servlet-name>
        <servlet-class>controllers.controlpanel.RemoveItems</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RemoveItem</servlet-name>
        <url-pattern>/remove/item</url-pattern>
    </servlet-mapping>
</web-app>